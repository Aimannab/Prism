<!-- Generated by Prism -->
{{% FOR textStyle %}}
<style name="{{%textStyle.identity.snakecase%}}">
    <item name="android:textSize">{{%textStyle.fontSize%}}sp</item>
    <item name="android:fontFamily">@font/{{%textStyle.fontName|lowercase|replace(-,_)%}}</item>
    <item name="android:textColor">@color/{{%textStyle.color.identity.snakecase%}}</item>
</style>
{{% END textStyle %}}